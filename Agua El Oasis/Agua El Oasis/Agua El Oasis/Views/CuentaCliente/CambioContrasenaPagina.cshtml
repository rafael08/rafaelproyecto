@model Agua_El_Oasis.Models.Modelos_Sub___Sistema_Cuenta_Cliente.Cliente

@{
    ViewBag.Title = "CambioContrasenaPagina";
}

<h2>Cambio Contrasena</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Cliente</legend>

       
        <div class="editor-label">
            @Html.LabelFor(model => model.p_nombreUsuario)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.p_nombreUsuario)
            @Html.ValidationMessageFor(model => model.p_nombreUsuario)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.p_contrasenaPersona)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.p_contrasenaPersona)
            @Html.ValidationMessageFor(model => model.p_contrasenaPersona)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.p_confirmContrasena)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.p_confirmContrasena)
            @Html.ValidationMessageFor(model => model.p_confirmContrasena)
        </div>

        <p>
            <input type="submit" value="Guardar" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Volver al inicio", "Index")
</div>
